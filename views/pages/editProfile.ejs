<% include ../partials/header %>
<!-- END: Header-->

<!-- BEGIN: Page Main-->
<div id="main">
      <div class="container" id="edit">
        <div class="col s12 m12 l12">
          <div id="Form-advance" class="card card card-default scrollspy">
            <div class="card-content">
              <form class="col s12">
                <div class="row">
                  <div class="input-field col m6 s12">
                    <input id="name" type="text">
                    <label for="name">Name</label>
                  </div>
                  <div class="input-field col m6 s12">
                    <input id="username" type="text">
                    <label for="username">Username</label>
                  </div>
                </div>
                <div class="row">
                  <div class="input-field col s12 m6">
                    <select id="designation">
                      <option value="1">SDE-1</option>
                      <option value="2">SDE-2</option>
                      <option value="3">Team Manager</option>
                    </select>
                    <label>Materialize Select</label>
                  </div>

                  <div class="input-field col m6 s12">
                    <input type="text" class="datepicker" id="dob"></input>
                    <label for="dob">DOB</label>
                  </div>

                </div>
                <div class="row">
                  <div class="container col s12 m6 center">
                    <div class="avatar-upload">
                        <div class="avatar-edit">
                            <input type='file' id="imageUpload" accept=".png, .jpg, .jpeg" />
                            <label for="imageUpload"></label>
                        </div>
                        <div class="avatar-preview center">
                            <div class="z-depth-5" id="imagePreview" style="background-image: url(http://i.pravatar.cc/500?img=7);">
                            </div>
                        </div>
                    </div>
                  </div>
                  <div class="container col s12 m6 center">
                    <div class="avatar-upload">
                        <div class="avatar-edit">
                            <input type='file' id="imageUpload-1" accept=".png, .jpg, .jpeg" />
                            <label for="imageUpload-1"></label>
                        </div>
                        <div class="avatar-preview center">
                            <div class="z-depth-5" id="imagePreview-1" style="background-image: url(http://i.pravatar.cc/500?img=7);">
                            </div>
                        </div>
                    </div>
                  </div>
                </div>
                <div class="row">
                  <div class="input-field col s12">
                    <a href="#" class="btn cyan waves-effect waves-light right" id="change"">change
                      <i class="material-icons right">edit</i>
                    </a>
                  </div>
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>
      <!-- START RIGHT SIDEBAR NAV -->
      <% include ../partials/rightsidemsgs %>
      <!-- END RIGHT SIDEBAR NAV -->

      </div>
    </div>
  </div>
</div>
<!-- END: Page Main-->

