<div class="section" id="user-profile">
  <div class="row">
    <!-- User Profile Feed -->
    <div class="col s12 m4 l3 user-section">
      <div class="row">
        <div class="col s12 center-align">
          <img class="responsive-img circle z-depth-5" width="200" src="<%=user.profilepicurl%>" alt="">
          <br>
        </div>
      </div>
      <div class="row mt-5">
        <div class="col s6 center-align">
          <h6>Followers</h6>
          <h5 class="m-1"><a href="#"><%=user.followers.length%></a></h5>
        </div>
        <div class="col s6 center-align">
          <h6>Following</h6>
          <h5 class="m-1"><a href="#"><%=user.following.length%></a></h5>
        </div>
      </div>
      <br></br>
      <hr>
      <div class="row mt-5">
        <div class="col s12">
          <h2 style="color:#173c54;font-weight:bold">Wish me on <%=user.dob%></h2>
        </div>
      </div>
      <hr>
      <div class="row mt-5">
        <div class="col s12">
          <h2 style="color:#173c54;font-weight:bold">Contact <%=user.username%></h2>
          <p class="latest-update mt-4">email - <%=user.email%></p>
          <p class="latest-update mt-2">chime - <%=user.username%></p>
          <p class="latest-update mt-2">Team -<%=user.team%></p>

        </div>
      </div>
      <hr>
    </div>
    <!-- User Post Feed -->
    <div class="col s12 m8 l6 mt-1">
      <div class="row">
        <div class="card user-card-negative-margin z-depth-0" id="feed">
          <div  class="card-content card-border-gray">
              <div id="tweet-input-out">
                <div class="row">
                  <div class="center mb-2">
                  <a class="waves-effect waves-light btn modal-trigger" href="#modal1">Post</a>
                  </div>
                    <!-- Modal Structure -->
                  <div id="modal1" class="modal">
                    <div class="modal-content">
                      <div class="center">
                      <h4>Share your thoughts ...</h4>
                      </div>
                      <div class="input-field col s12" id="tweet-input">
                        <textarea id="post-text" class="materialize-textarea"></textarea>
                        <label for="post-text">Share Your Thoughts</label>
                      </div>
                      <div class="input-field col s12">
                        <input id="post-tag" type="text">
                        <label for="post-tag">Add #tag to ypur post</label>
                      </div>
                      <div class="row">
                      <div class="avatar-upload col s12 m6 center-align">
                          <div class="avatar-edit" style="top:75px;right:75px">
                              <input type='file' id="imageUpload" accept=".png, .jpg, .jpeg, .avi, .flv, .wmv, .mov, .mp4" />
                              <label for="imageUpload"></label>
                          </div>
                          <div class="avatar-preview center" style="border-radius:5%">
                              <div class="z-depth-5" id="imagePreview" style="border-radius:5%;background:#eee">
                              </div>
                          </div>
                      </div>
                      <!-- <div class="avatar-upload col s12 m6 center-align">
                          <div class="avatar-edit" style="top:75px;right:75px">
                              <input type='file' id="videoUpload" accept=".avi, .flv, .wmv, .mov, .mp4" />
                              <label for="videoUpload"></label>
                          </div>
                          <div class="avatar-preview center" style="border-radius:5%">
                            <div class="video-container" style="border-radius:5%">
                              <iframe id ="videoPreview" width="853" height="480" src="https://www.youtube.com/embed/Q8TXgCzxEnw?rel=0" allowfullscreen></iframe>
                            </div>
                          </div>
                      </div> -->
                    </div>

                  </div>

                    <div class="modal-footer">

                      <a href="#!" id="sendbtn" class="modal-action waves-effect waves-green btn-flat ">Post</a>
                      <a href="#!" class="modal-action modal-close waves-effect waves-green btn-flat ">Cancel</a>
                    </div>

              </div>
              </div>
              </div>
              <div id="post-container"></div>

          </div>
        </div>
      </div>
